{
  "name": "@pearson-components/elementssdk",
  "version": "0.0.1",
  "description": "Styling and basic JavaScript components for UI development.\"",
  "main": "build/css/elements.css",
  "scripts": {
    "start": "npm run serve",
    "serve": "NODE_ENV=development webpack-dev-server --hot",
    "build": "NODE_ENV=production webpack -p",
    "test": "jest --no-cache --coverage",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "commit": "git-cz"
  },
  "jest": {
    "moduleFileExtensions": [
      "jsx",
      "js",
      "json"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "<rootDir>/test/__mocks__/styleMock.js",
      "^.+\\.(gif|ttf|eot|svg|png)$": "<rootDir>/test/__mocks__/fileMock.js"
    },
    "collectCoverageFrom": [
      "**/src/*.{js,jsx}",
      "**/src/**/*.{js,jsx}"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/(build|node_modules|demo|coverage)/",
      "<rootDir>/src/styles"
    ],
    "testEnvironment": "node",
    "coverageThreshold": {
      "global": {
        "lines": 80
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Pearson-Higher-Ed/ElementsSDK.git"
  },
  "dependencies": {},
  "devDependencies": {
    "@pearson-components/npm-scripts": "^0.5.1",
    "autoprefixer": "^7.1.4",
    "babel-core": "^6.23.1",
    "babel-eslint": "^8.0.0",
    "babel-jest": "^21.0.2",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.1.8",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.23.0",
    "browser-sync": "^2.10.0",
    "chai": "^4.1.2",
    "colors": "~1.1.2",
    "commitizen": "^2.9.6",
    "conventional-changelog-cli": "^1.3.3",
    "css-loader": "^0.28.7",
    "cz-conventional-changelog": "^2.0.0",
    "del": "^3.0.0",
    "enzyme": "^2.5.1",
    "eslint": "^4.6.1",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-react": "~7.3.0",
    "expect": "~21.0.2",
    "expect-jsx": "~3.0.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "highlight.js": "^9.12.0",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.28.0",
    "ignore-styles": "~5.0.1",
    "image-webpack-loader": "^3.2.0",
    "intl": "~1.2.5",
    "jest": "21.0.2",
    "jsdom": "^11.2.0",
    "markdown-it": "^8.4.0",
    "node-sass": "^4.5.0",
    "prop-types": "^15.5.6",
    "react": "^15.3.0",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.3.0",
    "react-intl": "~2.4.0",
    "react-phone-number-input": "^0.12.10",
    "react-router-dom": "^4.0.0-beta.6",
    "react-transition-group": "^2.2.0",
    "sass-loader": "^6.0.0",
    "semver": "~5.4.1",
    "sinon": "^3.2.1",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.7",
    "uuid": "3.1.0",
    "webpack": "^3.5.6",
    "webpack-dev-server": "^2.3.0"
  },
  "keywords": [
    "pearson-components"
  ],
  "author": "Pearson Design Accelerator",
  "license": "PEARSON PROPRIETARY AND CONFIDENTIAL INFORMATION SUBJECT TO NDA",
  "bugs": {
    "url": "https://github.com/Pearson-Higher-Ed/ElementsSDK/issues"
  },
  "homepage": "https://github.com/Pearson-Higher-Ed/ElementsSDK#readme",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
